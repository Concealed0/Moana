<Page x:Class="Moana.Pages.MonthChartPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Moana.Pages"
      xmlns:ChartControls="clr-namespace:Moana.Controls.ChartControls"
      mc:Ignorable="d" 
      d:DesignHeight="500" d:DesignWidth="500"
      Title="MonthChartPage">

    <Grid Background="#FFFFFFFF">
        <Grid.RowDefinitions>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>

        <ChartControls:MonthProjectStackedControl Grid.Row="0" Grid.Column="0" Margin="20"/>
    </Grid>
</Page>
